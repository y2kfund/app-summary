{
  "name": "@y2kfund/summary",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y2kfund/app-summary.git"
  },
  "homepage": "https://github.com/y2kfund/app-summary#readme",
  "bugs": {
    "url": "https://github.com/y2kfund/app-summary/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ag-grid-community": "^34.2.0",
    "ag-grid-vue3": "^34.2.0",
    "chart.js": "^4.5.0",
    "html2canvas": "^1.4.1",
    "tabulator-tables": "^6.3.1",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.3.2"
  },
  "peerDependencies": {
    "@y2kfund/core": "^0.1.0",
    "vue": "^3.4.0"
  },
  "_devDependenciesComment": "The devDependencies below use PRODUCTION versions from Git. These are used when npm install is run by cloudflare",
  "devDependencies": {
    "@tanstack/vue-query": "^5.0.0",
    "@types/node": "^24.4.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@y2kfund/core": "git+https://github.com/y2kfund/app-core.git",
    "rollup": "^4.0.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^2.0.0"
  },
  "scripts": {
    "_devComment": "Command '>npm run dev' for development with hot reload uses local package from ../app-core",
    "dev": "npm run deps:dev && vite",
    "_buildComment": "uses package from Git repository for app-core",
    "build": "npm run deps:prod && vite build",
    "build:lib": "npm run deps:prod && vite build --mode lib",
    "_previewComment": "Serves locally built files from dist/ folder. it doesn't build, just serves what's already in dist/",
    "preview": "vite preview",
    "_comment": "deps:dev installs local package temporarily (--no-save), deps:prod ensures Git repo is used",
    "deps:dev": "npm install @y2kfund/core@file:../app-core --no-save",
    "deps:prod": "npm install @y2kfund/core@git+https://github.com/y2kfund/app-core.git --no-save"
  }
}
